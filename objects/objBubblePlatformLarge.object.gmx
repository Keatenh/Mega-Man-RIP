    if timer &gt;= PopTime
    {
        image_index = image_number-2;
        yspeed = 0;
    }
    else
    {
        img += imgSpeed;
        if img &gt;= image_number-2
        {
            img = 4;
        }
        image_index = floor(img);
    }
    if timer &gt; PopTime+15
    {
        instance_destroy();
    }
    if place_meeting(x,y-2,prtPlayer) &amp;&amp; !Pop
    {
        if prtPlayer.bbox_bottom &lt; bbox_top &amp;&amp; prtPlayer.ground
        {
            image_index = 9;
            neverDespawn = true;
        }
    }
}
else
{
